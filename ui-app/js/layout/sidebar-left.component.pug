//- #rb-sidebar-left
a.rb-brand(href="main", target="_self")
	img.rb-brand-logo(src="/image/logotop_resize.png", alt="")
	img.rb-brand-logo-mini(src="/image/logo-mini_resize.png", alt="")
.rb-sidebar-menu-container(rb-sidebar-left)
	ul.rb-sidebar-menu
		li.rb-sidebar-label MENU
		li.rb-sidebar-menu-item
			a(routerLink="/")
				span Home
		li.rb-sidebar-menu-item
			a(routerLink="/hotel", [queryParams]="{ page: 1, size: 15 }")
				span Hotel list
		//- li.rb-sidebar-menu-item
			a(routerLink="/hotel/1/edit")
				span Hotel edit
		//- li.rb-sidebar-menu-item
			a.rb-sidebar-menu-toggler
				span Hotel
				i.arrow
			ul.rb-sidebar-sub-menu
				li: a(routerLink="/hotel", [queryParams]="{ page: 1, size: 15 }") Search
				li: a(routerLink="/hotel/1") View
		li.rb-sidebar-menu-item
			a(routerLink="/dashboard")
				span Dashboard
		li.rb-sidebar-menu-item
			a(routerLink="/not-create-this-page-yet")
				span No page

